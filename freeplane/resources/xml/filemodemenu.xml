<?xml version="1.0" encoding="UTF-8"?>
<menu_structure>
	<menu_category name="menu_bar">
		<menu_submenu name="file" name_ref="file">
			<menu_category name="close">
				<menu_action action="close" key_ref="keystroke_close" />
			</menu_category>
			<menu_separator />
			<menu_submenu name="last" name_ref="most_recent_files"></menu_submenu>
			<menu_category name="print">
				<menu_action action="page" />
				<menu_action action="print" key_ref="keystroke_print" />
				<menu_action action="printPreview" key_ref="keystroke_print_preview" />
			</menu_category>
			<menu_separator />
			<menu_category name="quit">
				<menu_action action="quit" key_ref="keystroke_quit" />
			</menu_category>
			<menu_separator />
		</menu_submenu>
		<menu_submenu name="edit" name_ref="edit">
			<menu_category name="find">
				<menu_action action="find" key_ref="keystroke_find" />
				<menu_action action="findNext" key_ref="keystroke_find_next" />
				<menu_action action="openPath"/>
			</menu_category>
		</menu_submenu>
		<menu_submenu name="view" name_ref="menu_view">
			<menu_category name="toolbars"></menu_category>
			<menu_action action="toggleToolbar" />
			<menu_separator />
			<menu_category name="general">
				<menu_action action="showSelectionAsRectangle" />
			</menu_category>
			<menu_action action="center_selected" key_ref="keystroke_center_selected" />
			<menu_category name="zoom">
				<menu_action action="zoomIn" key_ref="keystroke_zoom_in" />
				<menu_action action="zoomOut" key_ref="keystroke_zoom_out" />
			</menu_category>
		</menu_submenu>
		<menu_submenu name="navigate" name_ref="menu_navigate">
			<menu_category name="navigate">
				<menu_action action="navigationPreviousMap" key_ref="keystroke_previousMap" />
				<menu_action action="navigationNextMap" key_ref="keystroke_nextMap" />
			</menu_category>
			<menu_separator />
			<menu_category name="nodes">
				<menu_action action="moveToRoot" key_ref="keystroke_moveToRoot" />
			</menu_category>
			<menu_separator />
			<menu_category name="folding">
				<menu_action action="toggleFolded" key_ref="keystroke_toggle_folded" />
				<menu_action action="toggleChildrenFolded" key_ref="keystroke_toggle_children_folded" />
			</menu_category>
			<menu_separator />
		</menu_submenu>
		<menu_submenu name="mindmaps" name_ref="mindmaps">
			<menu_category name="modes"></menu_category>
			<menu_category name="navigate"></menu_category>
			<menu_separator />
			<menu_category name="mindmaps"></menu_category>
		</menu_submenu>
		<menu_submenu name="help" name_ref="help">
			<menu_category name="doc">
				<menu_action action="documentation" />
				<menu_action action="freeplaneUrl" />
				<menu_action action="faq" />
				<menu_action action="license" />
				<menu_action action="about" />
			</menu_category>
		</menu_submenu>
	</menu_category>
	<menu_category name="map_popup">
		<menu_category name="maps">
		</menu_category>
		<menu_separator />
		<menu_category name="toolbars">
			<menu_action action="toggleMenubar" />
			<menu_action action="toggleToolbar" />
		</menu_category>
	</menu_category>
	<menu_category name="node_popup">
			<menu_action action="center"/>
			<menu_separator />
			<menu_action action="find" key_ref="keystroke_find" />
			<menu_action action="findNext" key_ref="keystroke_find_next" />
		<menu_separator />
	</menu_category>
	<menu_category name="main_toolbar">
		<menu_category name="main">
			<menu_action action="navigationPreviousMap" />
			<menu_action action="navigationNextMap" />
			<menu_action action="printDirect" />
			<menu_action action="ShowFilterToolbarAction" tooltip="filter_toolbar" />
		</menu_category>
		<menu_category name="zoom">
		</menu_category>
		<menu_separator />
		<menu_action action="center" />
		<menu_category name="history"/>
	</menu_category>
</menu_structure>