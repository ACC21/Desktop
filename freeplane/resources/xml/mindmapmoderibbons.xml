<menu_structure>
	<menu_category name="ribbon">
		<ribbon_taskbar>
			<ribbon_action action="NewMapAction" accelerator="control N" />
			<ribbon_action action="EncryptedMap" />
			<separator />
			<ribbon_action action="SaveAction" accelerator="control S" />
			<ribbon_action action="SaveAsAction" accelerator="control shift S" />
			<ribbon_action action="SaveAll" />
		</ribbon_taskbar>
		<ribbon_menu>
			<primary_entry name="new">
				<entry_group name="new">
					<ribbon_action action="NewMapAction" accelerator="control N" />
					<ribbon_action action="EncryptedMap" />
				</entry_group>
			</primary_entry>
			<primary_entry name="save">
				<entry_group name="save">
					<ribbon_action action="SaveAction" accelerator="control S" />
					<ribbon_action action="SaveAsAction" accelerator="control shift S" />
					<ribbon_action action="SaveAll" />
				</entry_group>
			</primary_entry>
			<footer_entry action="QuitAction" />
		</ribbon_menu>
		<ribbon_task name="home">
			<ribbon_band name="mind_map" resize_policies="Mirror, High2Mid">
				<ribbon_action action="NewChildAction" accelerator="INSERT"
					priority="top">
					<ribbon_action action="NewSiblingAction" accelerator="ENTER" />
					<ribbon_action action="NewSummaryAction" />
					<separator />
					<ribbon_action action="NewFreeNodeAction" />
					<ribbon_action action="NewPreviousSiblingAction"
						accelerator="shift ENTER" />
					<ribbon_action action="NewParentNode" accelerator="shift INSERT" />
				</ribbon_action>
				<ribbon_action action="DeleteAction" accelerator="DELETE"
					priority="top" />
				<ribbon_action action="EditAction" priority="medium">
					<ribbon_action action="EditLongAction" accelerator="alt ENTER" />
				</ribbon_action>
				<ribbon_action action="JoinNodesAction" accelerator="control J"
					priority="low">
					<ribbon_action action="SplitNode" />
				</ribbon_action>
				<ribbon_action action="EditDetailsAction" priority="medium">
					<ribbon_action action="EditDetailsInDialogAction" />
					<ribbon_action action="DeleteDetailsAction" />
				</ribbon_action>
			</ribbon_band>

			<ribbon_band name="navigate" resize_policies="Mirror, High2Mid">
				<ribbon_action action="UnfoldAllAction" accelerator="alt END"
					priority="top" />
				<ribbon_action action="FoldAllAction" accelerator="alt HOME"
					priority="top" />
				<ribbon_action action="NavigationPreviousMapAction"
					accelerator="control shift TAB" priority="low" />
				<ribbon_action action="NavigationNextMapAction"
					accelerator="control TAB" priority="low" />
				<ribbon_action action="ToggleFoldedAction"
					accelerator="SPACE" priority="medium">
					<ribbon_action action="ToggleChildrenFoldedAction"
						accelerator="control SPACE" />
					<ribbon_action action="UnfoldOneLevelAction"
						accelerator="alt PAGE_DOWN" />
					<ribbon_action action="FoldOneLevelAction"
						accelerator="alt PAGE_UP" />
				</ribbon_action>
			</ribbon_band>

			<ribbon_band name="basics" resize_policies="Mirror, High2Mid">
				<ribbon_action action="CutAction" accelerator="control X"
					priority="top" />
				<ribbon_action action="CopyAction" accelerator="control C"
					priority="top">
					<ribbon_action action="CopySingleAction" accelerator="control shift C" />
					<ribbon_action action="CopyIDAction" />
					<ribbon_action action="CopyNodeURIAction" />
				</ribbon_action>
				<ribbon_action action="PasteAction" accelerator="control V"
					priority="top">
					<ribbon_action action="SelectedPasteAction" priority="top" />
				</ribbon_action>
				<ribbon_action action="UndoAction" accelerator="control Z"
					priority="top" />
				<ribbon_action action="RedoAction" accelerator="control Y"
					priority="top" />
			</ribbon_band>

			<ribbon_band name="attachments" resize_policies="Mirror, High2Mid">
				<ribbon_action action="AddLocalLinkAction"
					accelerator="alt shift L" type="dropdown" priority="top">
					<ribbon_action action="SetLinkByFileChooserAction" />
					<ribbon_action action="SetLinkByTextFieldAction" />
					<ribbon_action action="ExtractLinkFromTextAction" />
					<ribbon_action action="AddMenuItemLinkAction" />
					<ribbon_action action="SetLinkAnchorAction" />
					<ribbon_action action="MakeLinkFromAnchorAction" />
					<ribbon_action action="MakeLinkToAnchorAction" />
					<ribbon_action action="ClearLinkAnchorAction" />
				</ribbon_action>
				<ribbon_action name="add_node_attachement" priority="top">
					<ribbon_action action="ExternalImageAddAction" />
					<ribbon_action action="SetImageByFileChooserAction"
						accelerator="alt shift K" />
					<ribbon_action action="ExternalImageChangeAction" />
					<ribbon_action action="ExternalImageRemoveAction" />
					<ribbon_action action="LatexInsertLatexAction"
						plugin="org.freeplane.plugin.latex" priority="medium" />
					<ribbon_action action="LatexEditLatexAction" plugin="org.freeplane.plugin.latex" />
					<ribbon_action action="LatexDeleteLatexAction"
						plugin="org.freeplane.plugin.latex" />
					<ribbon_action action="OpenMapsAddLocation" plugin="org.freeplane.plugin.openmaps"
						priority="low" />
					<ribbon_action action="OpenMapsRemoveLocation"
						plugin="org.freeplane.plugin.openmaps" />
					<ribbon_action action="OpenMapsViewLocation" plugin="org.freeplane.plugin.openmaps" />
				</ribbon_action>
			</ribbon_band>

			<ribbon_band name="attributes" resize_policies="Mirror, High2Mid">
				<ribbon_action action="EditAttributesAction"
					accelerator="alt F9">
					<ribbon_action action="attributes_AddAttributeAction" />
					<ribbon_action action="AssignAttributesAction" />
					<ribbon_action action="CopyStyleExtensionsAction" />
					<ribbon_action action="CopyAttributes" />
					<ribbon_action action="PasteAttributes" />
					<ribbon_action action="AddStyleAttributes" />
					<ribbon_action action="attributes_RemoveFirstAttributeAction">
						<ribbon_action action="attributes_RemoveLastAttributeAction" />
						<ribbon_action action="attributes_RemoveAllAttributesAction" />
					</ribbon_action>
				</ribbon_action>
				<ribbon_action action="ShowAttributeDialogAction" />

			</ribbon_band>
		</ribbon_task>

		<ribbon_task name="formatting">
			<ribbon_contributor name="fontStyleContributor" />

			<ribbon_band name="manage styles" resize_policies="Mirror, High2Mid">

				<ribbon_action action="NewUserStyleAction" priority="medium">
					<ribbon_action action="RedefineStyleAction" />
					<ribbon_action action="ManageConditionalStylesAction" />
					<ribbon_action action="ManageNodeConditionalStylesAction" />
				</ribbon_action>

				<ribbon_action action="CopyMapStylesAction" />
				<ribbon_action action="EditStylesAction" />

				<ribbon_action action="FormatCopy" accelerator="alt shift C"
					priority="medium" />
				<ribbon_action action="FormatPaste" accelerator="alt shift V"
					priority="medium" />

				<ribbon_action action="AutomaticLayoutControllerAction.HEADINGS"
					priority="medium">
					<ribbon_action action="AutomaticLayoutControllerAction.ALL" />
					<ribbon_action action="AutomaticLayoutControllerAction.null" />
				</ribbon_action>
			</ribbon_band>

			<ribbon_band name="grouping" resize_policies="Mirror, High2Mid">
				<ribbon_action action="CloudAction">
					<ribbon_action action="CloudShapeAction.ARC" />
					<ribbon_action action="CloudShapeAction.STAR" />
					<ribbon_action action="CloudShapeAction.RECT" />
					<ribbon_action action="CloudShapeAction.ROUND_RECT" />
				</ribbon_action>
				<ribbon_action action="CloudColorAction" />
			</ribbon_band>

			<ribbon_contributor name="edgeStyleContributor" />

			<ribbon_band name="miscellaneous" resize_policies="High2Low">
				<ribbon_action action="RevisionPluginAction" priority="top" />
				<ribbon_action action="CopyStyleExtensionsAction" priority="top" />
			</ribbon_band>
		</ribbon_task>

		<ribbon_task name="view">
		    <ribbon_contributor	name="zoomContributor" /> 
			
		</ribbon_task>

	</menu_category>
</menu_structure>
