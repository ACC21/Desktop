<?xml version="1.0"?>
<export_with_xslt>
    <action
        tooltip="accessories/plugins/ExportWithXSLT.properties_documentation"
        name="accessories/plugins/ExportWithXSLT.properties_name"
        location="/menu_bar/file/export/export"
    >
        <properties
            file_type="user" />
    </action>
    <action
        tooltip="accessories/plugins/ExportWithXSLT_HTML.properties_documentation"
        name="accessories/plugins/ExportWithXSLT_HTML.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/accessories/toxhtml.xsl"
            create_dir="true"
            files_to_copy="marktree.js,minus.png,plus.png,ilink.png,treestyles.css"
            file_prefix="/accessories/"
            copy_icons="true"
            load_file="true"
            create_html_linked_image="false" />
    </action>
    <action
        tooltip="accessories/plugins/ExportWithXSLT_HTML3.properties_documentation"
        name="accessories/plugins/ExportWithXSLT_HTML3.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/accessories/freeplane2html.xsl"
            create_dir="true"
            files_to_copy="freeplane2html.css,hide.png,leaf.png,show.png,ilink.png"
            file_prefix="/accessories/"
            copy_icons="true"
            load_file="true"
            create_html_linked_image="true"/>
    </action>
    <action
        tooltip="accessories/plugins/ExportWithXSLT_Applet.properties_documentation"
        name="accessories/plugins/ExportWithXSLT_Applet.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/accessories/freeplane2applet.xsl"
            create_dir="true"
            files_to_copy="freeplaneviewer.jar"
            file_prefix="/lib/"
            copy_map="true"
            load_file="true" />
    </action>
    <action
        tooltip="accessories/plugins/ExportWithXSLT_Flash.properties_documentation"
        name="accessories/plugins/ExportWithXSLT_Flash.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="html"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_webpage"
            xslt_file="/accessories/freeplane2flash.xsl"
            create_dir="true"
            files_to_copy="visorFreeplane.swf,flashobject.js"
            file_prefix="/accessories/plugins/flash/"
            copy_map="true"
            load_file="true" />
    </action>
    <action
        tooltip="accessories/plugins/ExportWithXSLT_TASKSTJI.properties_documentation"
        name="accessories/plugins/ExportWithXSLT_TASKSTJI.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="tji"
            name_extension=".tasks"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_tji"
            xslt_file="/accessories/mm2tjitasks.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
    <action
        tooltip="accessories/plugins/ExportWithXSLT_RESOURCESTJI.properties_documentation"
        name="accessories/plugins/ExportWithXSLT_RESOURCESTJI.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="tji"
            name_extension=".resources"
            file_description="accessories/plugins/ExportWithXSLT_Applet.properties_tji"
            xslt_file="/accessories/mm2tjiresources.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
    <action
        tooltip="accessories/plugins/ExportWithTWiki.properties_documentation"
        name="accessories/plugins/ExportWithTWiki.properties_name"
        location="/menu_bar/file/export/export/html"
    >
        <properties
            file_type="twi"
            name_extension=""
            file_description="accessories/plugins/ExportWithTWiki.properties"
            xslt_file="/accessories/mm2twiki.xsl"
            copy_map="false"
            load_file="false"
            create_dir="false"
            create_html_linked_image="false" />
    </action>
</export_with_xslt>
