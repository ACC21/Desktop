<?xml version="1.0" encoding="UTF-8"?>
<preferences_structure>
<tabbed_pane>
    <tab name="Environment">
        <separator name="language">
       	    <combo name = "language">
                <choice value="automatic"/>
                <choice value="ar"/>
                <choice value="cs"/>
                <choice value="de"/>
                <choice value="dk"/>
                <choice value="en"/>
                <choice value="el"/>
                <choice value="es"/>
                <choice value="et"/>
                <choice value="fr"/>
                <choice value="gl"/>
                <choice value="hr"/>
                <choice value="hu"/>
                <choice value="id"/>
                <choice value="it"/>
                <choice value="ja"/>
                <choice value="kr"/>
                <choice value="lt"/>
                <choice value="nl"/>
                <choice value="nn"/>
                <choice value="nb"/>
                <choice value="pl"/>
                <choice value="pt_BR"/>
                <choice value="pt_PT"/>
                <choice value="ru"/>
                <choice value="sk"/>
                <choice value="se"/>
                <choice value="sl"/>
                <choice value="tr"/>
                <choice value="uk_UA"/>
                <choice value="vi"/>
                <choice value="zh_TW"/>
                <choice value="zh_CN"/>
            </combo>
        </separator>
        <separator name = "files">
            <string name = "last_opened_list_length"/> 
            <boolean name = "load_last_map"/>
            <boolean name = "experimental_file_locking_on"/>
            <string name = "userproperties"/>
            <string name = "patternsfile"/>
            <string name = "browsemode_initial_map"/>
        </separator>
        <separator name = "automatic_save">
            <string name = "time_for_automatic_save"/> 
            <boolean name = "delete_automatic_saves_at_exit"/>
            <string name = "number_of_different_files_for_automatic_save"/>
            <string name = "path_to_automatic_saves"/>
        </separator>
        <separator name = "save">
            <boolean name = "save_folding_state"/> 
            <boolean name = "save_only_intrisically_needed_ids"/>
        </separator>
    </tab>
    <tab name="Defaults">
        <separator name="default_styles">
            <combo name="standardnodeshape">
                <choice value="fork"/>
                <choice value="bubble"/>
                <choice value="as_parent"/>
                <choice value="combined"/>
            </combo>
            <combo name="standardrootnodeshape">
                <choice value="fork"/>
                <choice value="bubble"/>
                <choice value="combined"/>
            </combo>
        </separator>
         <separator name="default_colors">
            <color name="standardnodetextcolor"/>
            <color name="standardedgecolor"/>
            <color name="standardlinkcolor"/>
            <color name="standardbackgroundcolor"/>
            <boolean name="printonwhitebackground"/>
            <color name="standardcloudcolor"/>
         </separator>
        <separator name="default_fonts">
            <string name="defaultfont"/>
            <string name="defaultfontstyle"/>
            <number name="max_node_width"/>
            <number name="max_tooltip_width"/>
            <string name="standardbackgroundcolor"/>
            <boolean name="printonwhitebackground"/>
            <string name="standardcloudcolor"/>
         </separator>
        <separator name="other_defaults">
            <combo name="standardedgestyle">
                <choice value="bezier"/>
                <choice value="linear"/>
            </combo>
        </separator>
    </tab>
    <tab name="Appearance">
        <separator name="look_and_feel">
            <group name="lookandfeel"/>
            <boolean name="use_tabbed_pane"/>
        </separator>
        <separator name="selection_colors">        
            <boolean name="standarddrawrectangleforselection"/>
            <color name="standardselectednoderectanglecolor"/>
            <color name="standardselectednodecolor"/>
        </separator>
        <separator name="root_node_appearance">        
            <boolean name="use_common_out_point_for_root_node"/>
        </separator>
        <separator name="anti_alias">        
            <combo name="antialias">
                <choice value="antialias_edges"/>
                <choice value="antialias_all"/>
                <choice value="antialias_none"/>
            </combo>
        </separator>
        <separator name="initial_map_size">        
            <string name="mapxsize"/>
            <string name="mapysize"/>
        </separator>
        <separator name="hyperlink_types">        
            <combo name="links">
                <choice value="relative"/>
                <choice value="absolute"/>
            </combo>
            <string name="mapysize"/>
        </separator>
       <separator name="edit_long_node_window">        
            <string name="el__buttons_position"/>
            <boolean name="el__position_window_below_node" />
            <string name="el__min_default_window_height" />
            <string name="el__max_default_window_height" />
            <string name="el__min_default_window_width" />
            <string name="el__max_default_window_width" />
            <boolean name="el__enter_confirms_by_default" />
            <boolean name="el__show_icon_for_attributes" />
        </separator>
       <separator name="icon_properties">        
            <boolean name="don_t_show_note_icons" />
            <string name="icon_order_description"/>
        </separator>
    </tab>
    <tab name="Keystrokes" layout="right:max(40dlu;p), 4dlu, 80dlu, 7dlu,right:max(40dlu;p), 4dlu, 80dlu, 7dlu">
        <separator name="commands_for_the_program">
            <key name="keystroke_newMap"/>
            <key name="keystroke_open"/>
            <key name="keystroke_save"/>
            <key name="keystroke_saveAs"/>
            <key name="keystroke_print"/>
            <key name="keystroke_close"/>
            <key name="keystroke_quit"/>
            <key name="keystroke_option_dialog"/>
            <key name="keystroke_export_to_html"/>
            <key name="keystroke_export_branch_to_html"/>
            <key name="keystroke_open_first_in_history"/>
            <key name="keystroke_previousMap"/>
            <key name="keystroke_nextMap"/>
            <key name="keystroke_mode_MindMap"/>
            <key name="keystroke_mode_Browse"/>
            <key name="keystroke_mode_File"/>
            <key name="keystroke_node_toggle_italic"/>
            <key name="keystroke_node_toggle_boldface"/>
            <key name="keystroke_node_toggle_underlined"/>
            <key name="keystroke_node_toggle_cloud"/>
            <key name="keystroke_undo"/>
            <key name="keystroke_redo"/>
            <key name="keystroke_delete_child"/>
            <key name="keystroke_select_all"/>
            <key name="keystroke_select_branch"/>
            <key name="keystroke_zoom_out"/>
            <key name="keystroke_zoom_in"/>
        </separator>
        <separator name="node_editing_commands">
            <key name="keystroke_cut"/>
            <key name="keystroke_copy"/>
            <key name="keystroke_copy_single"/>
            <key name="keystroke_paste"/>
            <key name="keystroke_remove"/>
            <key name="keystroke_add_arrow_link_action"/>
            <key name="keystroke_add_local_link_action"/>
        </separator>
        <separator name="node_navigation_commands">
            <key name="keystroke_moveToRoot"/>
            <key name="keystroke_move_up"/>
            <key name="keystroke_move_down"/>
            <key name="keystroke_move_left"/>
            <key name="keystroke_move_right"/>
            <key name="keystroke_follow_link"/>
        </separator>
        <separator name="new_node_commands">
            <key name="keystroke_add"/>
            <key name="keystroke_add_child"/>
            <key name="keystroke_add_child_mac"/>
            <key name="keystroke_add_sibling_before"/>
        </separator>
        <separator name="node_editing_commands">
            <key name="keystroke_edit"/>
            <key name="keystroke_edit_long_node"/>
            <key name="keystroke_join_nodes"/>
            <key name="keystroke_toggle_folded"/>
            <key name="keystroke_toggle_children_folded"/>
            <key name="keystroke_set_link_by_filechooser"/>
            <key name="keystroke_set_link_by_textfield"/>
            <key name="keystroke_set_image_by_filechooser"/>
            <key name="keystroke_node_up"/>
            <key name="keystroke_node_down"/>
            <key name="keystroke_node_increase_font_size"/>
            <key name="keystroke_node_decrease_font_size"/>
            <key name="keystroke_export_branch"/>
            <key name="keystroke_node_color"/>
            <key name="keystroke_node_color_blend"/>
            <key name="keystroke_edge_color"/>
            <key name="keystroke_find"/>
            <key name="keystroke_find_next"/>
        </separator>
        <separator name="patterns">
    		<key name="keystroke_accessories/plugins/ManagePatterns_manage_patterns_dialog"/>
    		<key name="keystroke_apply_pattern_1"/>
    		<key name="keystroke_apply_pattern_2"/>
    		<key name="keystroke_apply_pattern_3"/>
    		<key name="keystroke_apply_pattern_4"/>
    		<key name="keystroke_apply_pattern_5"/>
    		<key name="keystroke_apply_pattern_6"/>
    		<key name="keystroke_apply_pattern_7"/>
    		<key name="keystroke_apply_pattern_8"/>
    		<key name="keystroke_apply_pattern_9"/>
    		<key name="keystroke_apply_pattern_10"/>
    		<key name="keystroke_apply_pattern_11"/>
    		<key name="keystroke_apply_pattern_12"/>
    		<key name="keystroke_apply_pattern_13"/>
    		<key name="keystroke_apply_pattern_14"/>
    		<key name="keystroke_apply_pattern_15"/>
    		<key name="keystroke_apply_pattern_16"/>
    		<key name="keystroke_apply_pattern_17"/>
    		<key name="keystroke_apply_pattern_18"/>
        </separator>
        <separator name="others">
            <key name="keystroke_accessories/plugins/ChangeNodeLevelAction_left.properties_key"/>
            <key name="keystroke_accessories/plugins/ChangeNodeLevelAction_right.properties_key"/>
            <key name="keystroke_accessories/plugins/FormatCopy.properties.properties_key"/>
            <key name="keystroke_accessories/plugins/FormatPaste.properties.properties_key"/>
            <key name="keystroke_accessories/plugins/IconSelectionPlugin.properties.properties_key"/>
            <key name="keystroke_accessories/plugins/NewParentNode.properties_key"/>
            <key name="keystroke_accessories/plugins/NodeNote_jumpto.keystroke.alt_N"/>
            <key name="keystroke_accessories/plugins/NodeNote_hide_show.keystroke.control_shift_less"/>
            <key name="keystroke_accessories/plugins/RemoveNote.properties.properties_key"/>
            <key name="keystroke_accessories/plugins/UnfoldAll.keystroke.alt_PAGE_UP"/>
            <key name="keystroke_accessories/plugins/UnfoldAll.keystroke.alt_PAGE_DOWN"/>
            <key name="keystroke_accessories/plugins/UnfoldAll.keystroke.alt_HOME"/>
            <key name="keystroke_accessories/plugins/UnfoldAll.keystroke.alt_END"/>
        </separator>
        <separator name="attributes">
            <key name="keystroke_edit_attributes"/>
            <key name="keystroke_show_all_attributes"/>
            <key name="keystroke_show_selected_attributes"/>
            <key name="keystroke_hide_all_attributes"/>
            <key name="keystroke_show_attribute_manager"/>
            <key name="keystroke_assign_attributes"/>
            <key name="keystroke_plugins/ScriptingEngine.keystroke.evaluate"/>
        </separator>
        <separator name="icons">
        </separator>
    </tab>
    <tab name="Behaviour">
        <separator name="behaviour">
            <combo name="placenewbranches">
                <choice value="first"/>
                <choice value="last"/>
            </combo>
            <boolean name="draganddrop"/>
            <boolean name="unfold_on_paste"/>
            <boolean name="disable_cursor_move_paper"/>
            <boolean name="enable_leaves_folding"/>
            <string name="foldingsymbolwidth"/>
        </separator>
        <separator name="key_typing">
            <boolean name="disable_key_type"/>
            <boolean name="key_type_adds_new"/>
       </separator>
        <separator name="notifications">
            <remind_value name="remind_use_rich_text_in_new_long_nodes"/>
            <dont_show_notification_property name="delete_nodes_without_question"/>
            <dont_show_notification_property name="cut_nodes_without_question"/>
            <dont_show_notification_property name="remove_notes_without_question"/>
            <dont_show_notification_property name="execute_scripts_without_asking"/>
        </separator>
        <separator name="selection_method">
            <combo name="selection_method">
                <choice value="selection_method_direct"/>
                <choice value="selection_method_delayed"/>
                <choice value="selection_method_by_click"/>
            </combo>
            <number name="time_for_delayed_selection"/>
        </separator>
        <separator name="mouse_wheel">
            <number name="wheel_velocity"/>
        </separator>
        <separator name="undo">
            <number name="undo_levels"/>
        </separator>
    </tab>
    <tab name="HTML">
        <separator name="browser">
            <string name="default_browser_command_windows_nt"/>
            <string name="default_browser_command_windows_9x"/>
            <string name="default_browser_command_other_os"/>
            <string name="default_browser_command_mac"/>
        </separator>
        <separator name="html_export">
            <combo name="html_export_folding">
                <choice value="html_export_no_folding"/>
                <choice value="html_export_fold_currently_folded"/>
                <choice value="html_export_fold_all"/>
                <choice value="html_export_based_on_headings"/>
            </combo>
            <boolean name="export_icons_in_html"/>
        </separator>
    </tab>
</tabbed_pane>
</preferences_structure>
